pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'I am building'
            }
        }
        stage('Test') {
            steps {
                echo 'I am testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'I am deploying'
            }
        }
        stage('Deploy again') {
            steps {
                echo 'i am at last!!!!!!!!!!!!'
            }
        }
    }
}
